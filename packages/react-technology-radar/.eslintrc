{
  "root": true,
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": ["babel"],
  "env": {
    "es6": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "16.14"
    }
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
